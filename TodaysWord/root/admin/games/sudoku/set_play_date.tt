<input type="text" name="play_date" class="datepicker_input" id="play_date" value="[% date %]" />
<a href="/admin/games/sudoku/set_play_date/[% sudoku.id %]" id="set_played_date_button_[% sudoku.id %]" class="admin_quick_button set_played_date" title="set_played_da$

<script type="text/javascript" language="JavaScript">
window.addEvent('domready', function() {

    $('set_played_date_button_[% sudoku.id %]').addEvent('click', function(event){
        event.stop();
        adminSetPlayDate(this.id);
    });

    [% IF NOT sudoku.date_played %]
    new DatePicker('.datepicker_input', {
                allowEmpty: true
        });
    [% END %]
});
</script>

