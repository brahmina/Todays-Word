<h1>Crossword Paused</h1>
<br /><br /><br />
<a href="/games/crossword/resume/[% game_play.id %]" rel="ajax:true" class="resume" id="game_resume">Resume</a>

<script type="text/javascript" language="JavaScript">
window.addEvent('domready', function() {
    $('game_resume').addEvent('click', function(event){
        event.stop();
        closeGamePause();
    });
});
</script>
